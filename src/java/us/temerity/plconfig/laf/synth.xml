<synth>
  <style id="Default">
    <state>
      <color value="#ffffff" type="FOREGROUND"/>
      <color value="#7f7f7f" type="BACKGROUND"/>
      <color value="#ffffff" type="TEXT_FOREGROUND"/>
      <color value="#7f7f7f" type="TEXT_BACKGROUND"/>
    </state>
  </style>
  <bind style="Default" type="region" key=".*"/>

  <style id="DefaultFont">
    <font name="Lucida Sans Bold" size="12"/>
  </style>
  <bind style="DefaultFont" type="name" key=".*"/>


  <!--  D I A L O G S  -->

  <style id="DialogHeader">
    <imagePainter path="DialogHeader.png"	
                  sourceInsets="0 3 6 3"/>
    <insets top="15" bottom="21" right="15" left="15"/>
  </style>
  <bind style="DialogHeader" type="name" key="DialogHeader"/>

  <style id="ModalDialogHeader">
    <imagePainter path="ModalDialogHeader.png"	
                  sourceInsets="0 3 6 3"/>
    <insets top="15" bottom="21" right="15" left="15"/>
  </style>
  <bind style="ModalDialogHeader" type="name" key="ModalDialogHeader"/>

  <style id="DialogHeaderLabel">
    <font name="Lucida Sans Demibold" size="16"/>
  </style>
  <bind style="DialogHeaderLabel" type="name" key="DialogHeaderLabel"/>

  <style id="DialogButtonPanel">
    <imagePainter path="DialogButtonPanel.png"	
                  sourceInsets="6 3 0 3"/>
    <insets top="21" bottom="15" right="15" left="15"/>
  </style>
  <bind style="DialogButtonPanel" type="name" key="DialogButtonPanel"/>


  <!--  M A I N   D I A L O G   P A N E L  -->

  <style id="MainDialogPanel">
    <insets top="20" bottom="20" right="20" left="20"/>
  </style>
  <bind style="MainDialogPanel" type="name" key="MainDialogPanel"/>


  <!--  D A R K   P A N E L  -->

  <style id="DarkPanel">
    <imagePainter path="DarkGrey.png" 
                  sourceInsets="0 0 0 0"/>
    <insets top="8" bottom="8" right="8" left="8"/>
  </style>
  <bind style="DarkPanel" type="name" key="DarkPanel"/>


  <!--  I N S E T   P A N E L   -->
 
  <style id="InsetPanel">
    <imagePainter path="FrameIn.png"
                  sourceInsets="3 3 3 3"/>
    <insets top="-3" bottom="-3" right="-3" left="-3"/>
  </style>
  <bind style="InsetPanel" type="name" key="InsetPanel"/>
   

  <!--  T I T L E   P A N E L  -->

  <style id="TitlePanel">
    <imagePainter path="TitlePanel.png" 
                  sourceInsets="0 0 0 1"/>
    <insets top="8" bottom="8" right="8" left="8"/>
  </style>
  <bind style="TitlePanel" type="name" key="TitlePanel"/>

  <style id="TopTitlePanel">
    <imagePainter path="TitlePanel.png" 
                  sourceInsets="0 0 0 1"/>
    <insets top="8" bottom="0" right="8" left="8"/>
  </style>
  <bind style="TopTitlePanel" type="name" key="TopTitlePanel"/>

  <style id="BottomTitlePanel">
    <imagePainter path="TitlePanel.png" 
                  sourceInsets="0 0 0 1"/>
    <insets top="0" bottom="8" right="8" left="8"/>
  </style>
  <bind style="BottomTitlePanel" type="name" key="BottomTitlePanel"/>


  <!--  V A L U E   P A N E L  -->

  <style id="ValuePanel">
    <imagePainter path="Grey.png" 
                  sourceInsets="0 0 0 0"/>
    <insets top="8" bottom="8" right="8" left="8"/>
  </style>
  <bind style="ValuePanel" type="name" key="ValuePanel"/>

  <style id="TopValuePanel">
    <imagePainter path="Grey.png" 
                  sourceInsets="0 0 0 0"/>
    <insets top="8" bottom="0" right="8" left="8"/>
  </style>
  <bind style="TopValuePanel" type="name" key="TopValuePanel"/>

  <style id="BottomValuePanel">
    <imagePainter path="Grey.png" 
                  sourceInsets="0 0 0 0"/>
    <insets top="0" bottom="8" right="8" left="8"/>
  </style>
  <bind style="BottomValuePanel" type="name" key="BottomValuePanel"/>


  <!--  V A L U E   P A N E L   B U T T O N  -->

  <style id="ValuePanelButton">
    <state>
      <imagePainter path="ValuePanelButton.png"
                    method="buttonBackground"
                    sourceInsets="5 5 5 5"/>
    </state>
    <state value="PRESSED">
      <imagePainter path="ValuePanelButtonPressed.png"
                    method="buttonBackground"	
                    sourceInsets="5 5 5 5"/>
    </state>
    <state value="DISABLED">
      <imagePainter path="ValuePanelButtonDisabled.png"
                    method="buttonBackground"	
                    sourceInsets="5 5 5 5"/>
    </state>
    <insets top="2" bottom="2" right="8" left="8"/>
    <property key="Button.textShiftOffset" type="integer" value="1"/>  
  </style>
  <bind style="ValuePanelButton" type="name" key="ValuePanelButton"/>


  <!--  M E N U S  -->

  <style id="Menu">
    <state>
      <imagePainter path="MenuItem.png"
                    sourceInsets="4 8 4 0"/>
    </state>
    <state value="SELECTED">
      <color value="#ffff00" type="TEXT_FOREGROUND"/>
      <imagePainter path="MenuItemSelected.png"
                    sourceInsets="4 8 4 0"/>
    </state>
    <state value="DISABLED">
      <color value="#e5e5e5" type="TEXT_FOREGROUND"/>
      <imagePainter path="MenuItemDisabled.png"
                    sourceInsets="4 8 4 0"/>
    </state>    
    <imageIcon id="MenuIcon" path="MenuIcon.png"/>
    <property key="Menu.arrowIcon" value="MenuIcon"/>
    <insets top="4" bottom="4" right="2" left="8"/>
  </style>
  <bind style="Menu" type="region" key="Menu"/>

  <style id="MenuItem">
    <state>
      <imagePainter path="MenuItem.png"
                    sourceInsets="4 8 4 0"/>
    </state>
    <state value="MOUSE_OVER">
      <color value="#ffff00" type="TEXT_FOREGROUND"/>
      <imagePainter path="MenuItemSelected.png"
                    sourceInsets="4 8 4 0"/>
    </state>
    <state value="DISABLED">
      <color value="#e5e5e5" type="TEXT_FOREGROUND"/>
      <imagePainter path="MenuItemDisabled.png"
                    sourceInsets="4 8 4 0"/>
    </state>    
    <insets top="4" bottom="4" right="2" left="8"/>
  </style>
  <bind style="MenuItem" type="region" key="MenuItem"/>

  <style id="CheckBoxMenuItem">
    <state>
      <imagePainter path="CheckBoxMenuItem.png"
                    sourceInsets="17 12 0 0"/>
    </state>
    <state value="MOUSE_OVER">
      <color value="#ffff00" type="TEXT_FOREGROUND"/>
      <imagePainter path="CheckBoxMenuItem.png"
                    sourceInsets="17 12 0 0"/>
    </state>
    <state value="SELECTED AND MOUSE_OVER">
      <color value="#ffff00" type="TEXT_FOREGROUND"/>
      <imagePainter path="CheckBoxMenuItemSelected.png"
                    sourceInsets="17 12 0 0"/>
    </state>
    <state value="SELECTED">
      <imagePainter path="CheckBoxMenuItemSelected.png"
                    sourceInsets="17 12 0 0"/>
    </state>
    <state value="DISABLED">
      <color value="#e5e5e5" type="TEXT_FOREGROUND"/>
      <imagePainter path="CheckBoxMenuItemDisabled.png"
                    sourceInsets="17 12 0 0"/>
    </state>    
    <insets top="4" bottom="4" right="2" left="8"/>
  </style>
  <bind style="CheckBoxMenuItem" type="region" key="CheckBoxMenuItem"/>

  <style id="PopupMenu">
    <opaque value="false"/>
    <imagePainter path="Panel.png"
                  sourceInsets="5 5 5 5"/>
    <insets top="5" bottom="5" right="5" left="5"/>
  </style>
  <bind style="PopupMenu" type="region" key="PopupMenu"/>

  <style id="PopupMenuSeparator">
    <imagePainter path="Separator.png"
                  sourceInsets="2 4 2 4"/>
    <insets top="2" bottom="2" right="4" left="4"/>
  </style>
  <bind style="PopupMenuSeparator" type="region" key="PopupMenuSeparator"/>


  <!--  S P L I T   P A N E  -->

  <style id="SplitPaneHorizontalDivider">
    <imagePainter path="SplitPaneHorizontalDivider.png"
                  method="splitPaneDividerForeground"
                  direction="horizontal_split"
                  sourceInsets="4 4 4 4"/>
    <insets top="4" bottom="4" right="4" left="4"/>
  </style>
  <bind style="SplitPaneHorizontalDivider" type="region" key="SplitPaneDivider"/>

  <style id="SplitPaneHorizontalDividerLeftButton">
    <state>
      <imagePainter path="SplitPaneHorizontalDividerLeftButton.png"
                    sourceInsets="2 2 2 2"/>
    </state>
    <state value="PRESSED">
      <imagePainter path="SplitPaneHorizontalDividerLeftButtonPressed.png"
                    sourceInsets="2 2 2 2"/>
    </state>
    <insets top="2" bottom="2" right="2" left="2"/>
  </style>
  <bind style="SplitPaneHorizontalDividerLeftButton" type="name" 
        key="SplitPaneHorizontalDivider.leftOneTouchButton"/>

  <style id="SplitPaneHorizontalDividerRightButton">
    <state>
      <imagePainter path="SplitPaneHorizontalDividerRightButton.png"
                    sourceInsets="2 2 2 2"/>
    </state>
    <state value="PRESSED">
      <imagePainter path="SplitPaneHorizontalDividerRightButtonPressed.png"
                    sourceInsets="2 2 2 2"/>
    </state>
    <insets top="2" bottom="2" right="2" left="2"/>
  </style>
  <bind style="SplitPaneHorizontalDividerRightButton" type="name" 
        key="SplitPaneHorizontalDivider.rightOneTouchButton"/>


  <style id="SplitPaneVerticalDivider">
    <imagePainter path="SplitPaneVerticalDivider.png"
                  method="splitPaneDividerForeground"
                  direction="vertical_split"
                  sourceInsets="4 4 4 4"/>
    <insets top="4" bottom="4" right="4" left="4"/>
  </style>
  <bind style="SplitPaneVerticalDivider" type="region" key="SplitPaneDivider"/>

  <style id="SplitPaneVerticalDividerUpButton">
    <state>
      <imagePainter path="SplitPaneVerticalDividerUpButton.png"
                    sourceInsets="2 2 2 2"/> 
    </state>
    <state value="PRESSED">
      <imagePainter path="SplitPaneVerticalDividerUpButtonPressed.png"
                    sourceInsets="2 2 2 2"/>
    </state>
    <insets top="2" bottom="2" right="2" left="2"/>
  </style>
  <bind style="SplitPaneVerticalDividerUpButton" type="name" 
        key="SplitPaneVerticalDivider.upOneTouchButton"/>

  <style id="SplitPaneVerticalDividerDownButton">
    <state>
      <imagePainter path="SplitPaneVerticalDividerDownButton.png"
                    sourceInsets="2 2 2 2"/>
    </state>
    <state value="PRESSED">
      <imagePainter path="SplitPaneVerticalDividerDownButtonPressed.png"
                    sourceInsets="2 2 2 2"/>
    </state>
    <insets top="2" bottom="2" right="2" left="2"/>
  </style>
  <bind style="SplitPaneVerticalDividerDownButton" type="name" 
        key="SplitPaneVerticalDivider.downOneTouchButton"/>


  <!--  S P A C E R  -->

  <style id="Spacer">
    <imagePainter path="Spacer.png"
                  method="panelBackground"
                  sourceInsets="4 4 4 4"/>
  </style>
  <bind style="Spacer" type="name" key="Spacer"/>


  <!--  I T E M   L I S T   P A N E L  -->

  <style id="ItemListPanel">
    <imagePainter path="DrawerHeader.png"
                  sourceInsets="4 4 4 4"/>
    <insets top="4" bottom="4" right="4" left="4"/>
  </style>
  <bind style="ItemListPanel" type="name" key="ItemListPanel"/>


  <!--  S C R O L L   P A N E -->

  <style id="ScrollPane">
    <imagePainter path="FrameIn.png"
                  method="viewportBorder"
                  sourceInsets="3 3 3 3"/>
    <property key="ScrollPane.viewportBorderInsets" type="insets" value="3 3 3 3"/>	
  </style>
  <bind style="ScrollPane" type="region" key="ScrollPane"/>



  <!--  S C R O L L   B A R  -->

  <style id="ScrollBar">
    <property key="ScrollBar.allowsAbsolutePositioning" type="boolean" value="true"/>
  </style>
  <bind style="ScrollBar" type="region" key="ScrollBar"/>

  <style id="ScrollBarThumb">
    <state>
      <imagePainter path="ScrollBarThumb.png"
                    sourceInsets="4 4 4 4"/>
    </state>
  </style>
  <bind style="ScrollBarThumb" type="region" key="ScrollBarThumb"/>

  <style id="ScrollBarTrack"> 
    <imagePainter path="FrameInDark.png"
                  sourceInsets="3 3 3 3"/>
    <insets top="3" bottom="3" right="3" left="3"/>
  </style>
  <bind style="ScrollBarTrack" type="region" key="ScrollBarTrack"/>
   
  <style id="ScrollBarArrowNorth"> 
    <state>
      <imagePainter path="ScrollArrowNorth.png"
                    method="arrowButtonForeground"
                    direction="north"
                    sourceInsets="16 14 0 0"/>  
    </state>
    <state value="PRESSED">
      <imagePainter path="ScrollArrowNorthPressed.png"
                    method="arrowButtonForeground"
                    direction="north"
                    sourceInsets="16 14 0 0"/>  
    </state>
    <insets top="0" bottom="0" right="0" left="0"/>
  </style>
  <bind style="ScrollBarArrowNorth" type="name" key="ScrollBar.button"/>

  <style id="ScrollBarArrowSouth"> 
    <state>
      <imagePainter path="ScrollArrowSouth.png"
                    method="arrowButtonForeground"
                    direction="south"
                    sourceInsets="16 14 0 0"/> 
    </state>
    <state value="PRESSED">
      <imagePainter path="ScrollArrowSouthPressed.png"
                    method="arrowButtonForeground"
                    direction="south" 
                    sourceInsets="16 14 0 0"/> 
    </state>
    <insets top="0" bottom="0" right="0" left="0"/>
  </style>
  <bind style="ScrollBarArrowSouth" type="name" key="ScrollBar.button"/>

  <style id="ScrollBarArrowWest"> 
    <state>
      <imagePainter path="ScrollArrowWest.png"
                    method="arrowButtonForeground"
                    direction="west"
                    sourceInsets="14 16 0 0"/>  
    </state>
    <state value="PRESSED">
      <imagePainter path="ScrollArrowWestPressed.png"
                    method="arrowButtonForeground"
                    direction="west"
                    sourceInsets="14 16 0 0"/>  
    </state>
    <insets top="0" bottom="0" right="0" left="0"/>
  </style>
  <bind style="ScrollBarArrowWest" type="name" key="ScrollBar.button"/>

  <style id="ScrollBarArrowEast"> 
    <state>
      <imagePainter path="ScrollArrowEast.png"
                    method="arrowButtonForeground"
                    direction="east"
                    sourceInsets="14 16 0 0"/>  
    </state>
    <state value="PRESSED">
      <imagePainter path="ScrollArrowEastPressed.png"
                    method="arrowButtonForeground"
                    direction="east"
                    sourceInsets="14 16 0 0"/>  
    </state>
    <insets top="0" bottom="0" right="0" left="0"/>
  </style>
  <bind style="ScrollBarArrowEast" type="name" key="ScrollBar.button"/>


  <!--  R A I S E D    B U T T O N  -->

  <style id="RaisedButton">
    <state>
      <imagePainter path="RaisedButton.png"
                    method="buttonBackground"
                    sourceInsets="23 21 4 4"/>
    </state>
    <state value="PRESSED">
      <imagePainter path="RaisedButtonPressed.png"
                    method="buttonBackground"	
                    sourceInsets="23 21 4 4"/>
    </state>
    <state value="DISABLED">
      <color value="#e5e5e5" type="TEXT_FOREGROUND"/>
      <imagePainter path="RaisedButtonDisabled.png"
                    method="buttonBackground"	
                    sourceInsets="23 21 4 4"/>
    </state>
    <insets top="8" bottom="8" right="24" left="30"/> 
    <property key="Button.textShiftOffset" type="integer" value="1"/>  
  </style>
  <bind style="RaisedButton" type="name" key="RaisedButton"/>

  <style id="RaisedConfirmButton">
    <state>
      <imagePainter path="RaisedConfirmButton.png"
                    method="buttonBackground"
                    sourceInsets="23 21 4 4"/>
    </state>
    <state value="PRESSED">
      <imagePainter path="RaisedConfirmButtonPressed.png"
                    method="buttonBackground"	
                    sourceInsets="23 21 4 4"/>
    </state>
    <state value="DISABLED">
      <color value="#e5e5e5" type="TEXT_FOREGROUND"/>
      <imagePainter path="RaisedConfirmButtonDisabled.png"
                    method="buttonBackground"	
                    sourceInsets="23 21 4 4"/>
    </state>
    <insets top="8" bottom="8" right="24" left="30"/> 
    <property key="Button.textShiftOffset" type="integer" value="1"/>  
  </style>
  <bind style="RaisedConfirmButton" type="name" key="RaisedConfirmButton"/>

  <style id="RaisedCancelButton">
    <state>
      <imagePainter path="RaisedCancelButton.png"
                    method="buttonBackground"
                    sourceInsets="23 21 4 4"/>
    </state>
    <state value="PRESSED">
      <imagePainter path="RaisedCancelButtonPressed.png"
                    method="buttonBackground"	
                    sourceInsets="23 21 4 4"/>
    </state>
    <state value="DISABLED">
      <color value="#e5e5e5" type="TEXT_FOREGROUND"/>
      <imagePainter path="RaisedCancelButtonDisabled.png"
                    method="buttonBackground"	
                    sourceInsets="23 21 4 4"/>
    </state>
    <insets top="8" bottom="8" right="24" left="30"/> 
    <property key="Button.textShiftOffset" type="integer" value="1"/>  
  </style>
  <bind style="RaisedCancelButton" type="name" key="RaisedCancelButton"/>


  <!--  L E F T   B U T T O N  -->

  <style id="LeftButton">
    <state>
      <imagePainter path="LeftButton.png"
                    method="buttonBackground"
                    sourceInsets="24 24 4 4"/>
    </state>
    <state value="PRESSED">
      <imagePainter path="LeftButtonPressed.png"
                    method="buttonBackground"	
                    sourceInsets="24 24 4 4"/>
    </state>
    <state value="DISABLED">
      <color value="#e5e5e5" type="TEXT_FOREGROUND"/>
      <imagePainter path="LeftButtonDisabled.png"
                    method="buttonBackground"	
                    sourceInsets="24 24 4 4"/>
    </state>
    <insets top="8" bottom="8" right="16" left="16"/> 
    <property key="Button.textShiftOffset" type="integer" value="1"/>  
  </style>
  <bind style="LeftButton" type="name" key="LeftButton"/>


  <!--  R I G H T   B U T T O N  -->

  <style id="RightButton">
    <state>
      <imagePainter path="RightButton.png"
                    method="buttonBackground"
                    sourceInsets="24 5 4 23"/>
    </state>
    <state value="PRESSED">
      <imagePainter path="RightButtonPressed.png"
                    method="buttonBackground"	
                    sourceInsets="24 5 4 23"/>
    </state>
    <state value="DISABLED">
      <color value="#e5e5e5" type="TEXT_FOREGROUND"/>
      <imagePainter path="RightButtonDisabled.png"
                    method="buttonBackground"	
                    sourceInsets="24 5 4 23"/>
    </state>
    <insets top="8" bottom="8" right="16" left="16"/> 
    <property key="Button.textShiftOffset" type="integer" value="1"/>  
  </style>
  <bind style="RightButton" type="name" key="RightButton"/>


  <!--  B R O W S E   B U T T O N  -->

  <style id="BrowseButton"> 
    <state>
      <imagePainter path="BrowseButton.png"
                    sourceInsets="0 0 0 0"/>
    </state>
    <state value="DISABLED">
      <imagePainter path="BrowseButtonDisabled.png"
                    sourceInsets="0 0 0 0"/>
    </state>
    <state value="PRESSED">
      <imagePainter path="BrowseButtonPressed.png"
                    sourceInsets="0 0 0 0"/>
    </state>
  </style>
  <bind style="BrowseButton" type="name" key="BrowseButton"/>


  <!--  F O L D E R   B U T T O N  -->

  <style id="FolderButton"> 
    <state>
      <imagePainter path="FolderButton.png"
                    sourceInsets="0 0 0 0"/>
    </state>
    <state value="PRESSED">
      <imagePainter path="FolderButtonPressed.png"
                    sourceInsets="0 0 0 0"/>
    </state>
  </style>
  <bind style="FolderButton" type="name" key="FolderButton"/>


  <!--  H O M E   B U T T O N  -->

  <style id="HomeButton"> 
    <state>
      <imagePainter path="HomeButton.png"
                    sourceInsets="0 0 0 0"/>
    </state>
    <state value="PRESSED">
      <imagePainter path="HomeButtonPressed.png"
                    sourceInsets="0 0 0 0"/>
    </state>
  </style>
  <bind style="HomeButton" type="name" key="HomeButton"/>


  <!--  H E L P   B U T T O N  -->

  <style id="HelpButton"> 
    <state>
      <imagePainter path="Help.png"
                    sourceInsets="0 0 0 0"/>
    </state>
    <state value="PRESSED">
      <imagePainter path="HelpPressed.png"
                    sourceInsets="0 0 0 0"/>
    </state>
    <state value="DISABLED">
      <imagePainter path="HelpDisabled.png"
                    sourceInsets="0 0 0 0"/>
    </state>
  </style>
  <bind style="HelpButton" type="name" key="HelpButton"/>


  <!--  T E X T   F I E L D  -->

  <style id="TextField">
    <state value="SELECTED">
       <color value="#ffff00" type="TEXT_FOREGROUND"/>
       <color value="#727272" type="TEXT_BACKGROUND"/>
    </state>
    <imagePainter path="ValuePanel.png"
                  method="textFieldBackground"
                  sourceInsets="3 3 3 3"/>
    <insets top="2" bottom="2" right="8" left="8"/>
    <font name="Lucida Sans Bold" size="12"/>
  </style>
  <bind style="TextField" type="name" key="TextField"/>

  <style id="EditableTextField">
    <state value="SELECTED">
       <color value="#ffff00" type="TEXT_FOREGROUND"/>
       <color value="#727272" type="TEXT_BACKGROUND"/>
    </state>
    <state value="DISABLED"> 
      <imagePainter path="ValuePanel.png"
                    method="textFieldBackground"
                    sourceInsets="3 3 3 3"/>
    </state>
    <state> 
      <imagePainter path="EditableValuePanel.png"
                    method="textFieldBackground"
                    sourceInsets="3 3 3 3"/> 
    </state>
    <insets top="2" bottom="2" right="8" left="8"/>
    <font name="Lucida Sans Bold" size="12"/>
  </style>
  <bind style="EditableTextField" type="name" key="EditableTextField"/>


  <!--  B O O L E A N   F I E L D  -->

  <style id="BooleanField">
    <state value="DISABLED"> 
      <imagePainter path="ValuePanel.png"
                    sourceInsets="3 3 3 3"/>
    </state>
    <state> 
      <imagePainter path="EditableValuePanel.png"
                    sourceInsets="3 3 3 3"/> 
    </state>
    <insets top="2" bottom="2" right="2" left="2"/>
    <font name="Lucida Sans Bold" size="12"/>
  </style>
  <bind style="BooleanField" type="name" key="BooleanField"/>

  <style id="BooleanValueTextField">
    <state value="SELECTED">
       <color value="#ffffff" type="TEXT_FOREGROUND"/>
       <color value="#727272" type="TEXT_BACKGROUND"/>
    </state>
    <imagePainter path="DarkGrey.png"
                  method="textFieldBackgroud"
                  sourceInsets="0 0 0 0"/>
    <font name="Lucida Sans Bold" size="12"/>
  </style>
  <bind style="BooleanValueTextField" type="name" key="BooleanValueTextField"/>


  <!--  C O L L E C T I O N   F I E L D  -->

  <style id="CollectionField">
    <state value="DISABLED"> 
      <imagePainter path="ValuePanel.png"
                    sourceInsets="3 3 3 3"/>
    </state>
    <state> 
      <imagePainter path="EditableValuePanel.png"
                    sourceInsets="3 3 3 3"/> 
    </state>
    <insets top="2" bottom="2" right="2" left="2"/>
    <font name="Lucida Sans Bold" size="12"/>
  </style>
  <bind style="CollectionField" type="name" key="CollectionField"/>

  <style id="CollectionValueTextField">
    <state value="SELECTED">
       <color value="#ffff00" type="TEXT_FOREGROUND"/>
       <color value="#727272" type="TEXT_BACKGROUND"/>
    </state>
    <imagePainter path="DarkGrey.png"
                  method="textFieldBackgroud"
                  sourceInsets="0 0 0 0"/>
    <font name="Lucida Sans Bold" size="12"/>
  </style>
  <bind style="CollectionValueTextField" type="name" key="CollectionValueTextField"/>


  <!--  P A N E L   L A B E L  -->

  <style id="PanelLabel">
    <font name="Lucida Sans Bold" size="14"/>
  </style>
  <bind style="PanelLabel" type="name" key="PanelLabel"/>


  <!--  P A S S W O R D   F I E L D  -->

  <style id="PasswordField">
    <state value="SELECTED">
       <color value="#ffff00" type="TEXT_FOREGROUND"/>
       <color value="#727272" type="TEXT_BACKGROUND"/>
    </state>
    <state value="DISABLED"> 
      <imagePainter path="ValuePanel.png"
                    method="passwordFieldBackground"
                    sourceInsets="3 3 3 3"/>
    </state>
    <state> 
      <imagePainter path="EditableValuePanel.png"
                    method="passwordFieldBackground"
                    sourceInsets="3 3 3 3"/> 
    </state>
    <insets top="2" bottom="2" right="8" left="8"/>
    <font name="Lucida Sans Bold" size="12"/>
  </style>
  <bind style="PasswordField" type="name" key="PasswordField"/>


  <!--  T E X T   A R E A  -->

  <style id="TextArea">
    <state value="SELECTED">
       <color value="#ffff00" type="TEXT_FOREGROUND"/>
       <color value="#727272" type="TEXT_BACKGROUND"/>
    </state>
    <imagePainter path="ValuePanel.png"
                  method="textAreaBackground"
                  sourceInsets="3 3 3 3"/>
    <insets top="2" bottom="2" right="8" left="8"/>
    <font name="Lucida Sans Bold" size="12"/>
  </style>
  <bind style="TextArea" type="name" key="TextArea"/>

  <style id="EditableTextArea">
    <state value="SELECTED">
       <color value="#ffff00" type="TEXT_FOREGROUND"/>
       <color value="#727272" type="TEXT_BACKGROUND"/>
    </state>
    <imagePainter path="EditableValuePanel.png"
                  method="textAreaBackground"
                  sourceInsets="3 3 3 3"/> 
    <insets top="2" bottom="2" right="8" left="8"/>
    <font name="Lucida Sans Bold" size="12"/>
  </style>
  <bind style="EditableTextArea" type="name" key="EditableTextArea"/>

  <style id="CodeTextArea">
    <state value="SELECTED">
       <color value="#ffff00" type="TEXT_FOREGROUND"/>
       <color value="#727272" type="TEXT_BACKGROUND"/>
    </state>
    <imagePainter path="DarkGrey.png"
                  method="textAreaBackground"
                  sourceInsets="0 0 0 0"/>
    <insets top="2" bottom="2" right="8" left="8"/>
    <font name="Courier" size="16"/>
  </style>
  <bind style="CodeTextArea" type="name" key="CodeTextArea"/>

  <style id="ScrolledTextArea">
    <state value="SELECTED">
       <color value="#ffff00" type="TEXT_FOREGROUND"/>
       <color value="#727272" type="TEXT_BACKGROUND"/>
    </state>
    <imagePainter path="DarkGrey.png"
                  method="textAreaBackground"
                  sourceInsets="0 0 0 0"/>
    <insets top="2" bottom="2" right="8" left="8"/>
    <font name="Lucida Sans Bold" size="12"/>
  </style>
  <bind style="ScrolledTextArea" type="name" key="ScrolledTextArea"/>


  <!--  S L I D E R  --> 

  <style id="Slider">

    <property key="Slider.thumbWidth" type="integer" value="9"/>
    <property key="Slider.thumbHeight" type="integer" value="19"/>
    <property key="Slider.trackBorder" type="integer" value="0"/>
    <property key="Slider.paintValue" type="boolean" value="false"/>
  </style>
  <bind style="Slider" type="region" key="Slider"/>

  <style id="SliderTrack">
      <imagePainter path="SliderTrack.png"
    	            method="sliderTrackBackground"
                    sourceInsets="10 2 8 2"/>
  </style>
  <bind style="SliderTrack" type="region" key="SliderTrack"/>

  <style id="SliderThumb">
      <imagePainter path="SliderThumb.png"
                    sourceInsets="0 0 0 0"/>
  </style>
  <bind style="SliderThumb" type="region" key="SliderThumb"/>


  <!--  L I S T  -->

  <style id="List">
    <imagePainter path="DarkGrey.png"
                  method="listBackground"
                  sourceInsets="0 0 0 0"/>
    <insets top="0" bottom="0" right="4" left="4"/>
  </style>
  <bind style="List" type="region" key="List"/>


</synth>
